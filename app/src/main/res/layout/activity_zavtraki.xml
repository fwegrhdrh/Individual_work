        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:contentDescription="@string/pic"
            android:src="@drawable/back" />

    </LinearLayout>

    <LinearLayout
