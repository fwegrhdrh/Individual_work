            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/_140"
            android:textColor="@color/black"
            android:textSize="@dimen/size20"
            android:textStyle="bold" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout16"
