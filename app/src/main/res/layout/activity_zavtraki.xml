        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="10dp"
            android:contentDescription="@string/pic"
            android:src="@drawable/vpered" />

    </LinearLayout>

    <LinearLayout
